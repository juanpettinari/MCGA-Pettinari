@model MasVidaWebMVC.FamilyGroup

@{
    ViewBag.Title = "Grupos Familiares";
}

<h2>Detalles</h2>

<fieldset>
    <legend>Grupo Familiar</legend>

    <div class="display-label">
         <strong>Familia</strong>
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.FamilyName)
    </div>
</fieldset>
<br />
<p>
    @Html.ActionLink("Editar", "Edit", new { id = Model.FamilyGroupID }, new { @class = "btn btn-primary" })
    @Html.ActionLink("Volver", "Index", null, new { @class = "btn btn-primary" })
</p>